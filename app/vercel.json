{
    "version": 2,
    "builds": [
      {
        "src": "app.py",  
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/game/board",
        "dest": "./app/app.py"  
      },
      {
        "src": "/api/game/make_human_move",
        "dest": "./app/app.py"
      },
      {
        "src": "/api/game/make_computer_move",
        "dest": "./app/app.py"
      },
      {
        "src": "/api/game/possible_moves",
        "dest": "./app/app.py"
      },
      {
        "src": "/api/game/reset",
        "dest": "./app/app.py"
      }
    ]
  }
  